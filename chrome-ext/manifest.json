{
  "manifest_version": 3,
  "name": "IP提取器",
  "version": "1.0",
  "description": "IP提取器扩展，用于从网页中提取IP地址。",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "IP提取器"
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon16.png",
    "128": "icon16.png"
  }
}